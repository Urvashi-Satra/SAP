      entity: {
          name : 'ZMAKS_I_GENDER_M',
          element: 'GenderCode'
         }

   }]
  @UI.textArrangement: #TEXT_FIRST
  Gender;
  @UI.identification: [{ position: 60 ,label: 'DOB'   }]
  Dob;
  @UI.lineItem: [{ position: 70 ,label: 'Age' }]
  @UI.identification: [{ position: 70 ,label: 'Age'  }]
  Age;
  @UI.lineItem: [{ position: 80 ,label: 'Telephone'  }]
  @UI.identification: [{ position: 80 ,label: 'Telephone'  }]
  @Search.defaultSearchElement: true
  @Search.fuzzinessThreshold: 0.8
  Telephone;
  @UI.identification: [{ position: 90 ,label: 'Email'  }]
  Email;
  @UI.lineItem: [{ position:  100,label: 'Active' }]
  @UI.identification: [{ position: 100 ,label: 'Active'  }]
  Active;

  @UI.hidden: true
  CreatedBy;
  @UI.hidden: true
  CreatedAt;
  @UI.hidden: true
  LastChangedBy;
  @UI.hidden: true
  LastChangedAt;

}